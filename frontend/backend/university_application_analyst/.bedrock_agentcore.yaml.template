default_agent: uni_application_analyst
agents:
  uni_application_analyst:
    name: uni_application_analyst
    entrypoint: /path/to/your/uni-application-analyst.py
    platform: linux/arm64
    container_runtime: docker
    source_path: null
    aws:
      execution_role: arn:aws:iam::YOUR_AWS_ACCOUNT_ID:role/BedrockAgentCoreExecutionRole
      execution_role_auto_create: false
      account: YOUR_AWS_ACCOUNT_ID
      region: us-east-1
      ecr_repository: YOUR_AWS_ACCOUNT_ID.dkr.ecr.us-east-1.amazonaws.com/bedrock-agentcore-uni_application_analyst
      ecr_auto_create: false
      network_configuration:
        network_mode: PUBLIC
        network_mode_config: null
      protocol_configuration:
        server_protocol: HTTP
      observability:
        enabled: true
    bedrock_agentcore:
      agent_id: uni_application_analyst-YOUR_AGENT_ID
      agent_arn: arn:aws:bedrock-agentcore:us-east-1:YOUR_AWS_ACCOUNT_ID:runtime/uni_application_analyst-YOUR_AGENT_ID
      agent_session_id: null
    codebuild:
      project_name: bedrock-agentcore-uni_application_analyst-builder
      execution_role: arn:aws:iam::YOUR_AWS_ACCOUNT_ID:role/BedrockAgentCoreCodeBuildRole
      source_bucket: bedrock-agentcore-codebuild-sources-YOUR_AWS_ACCOUNT_ID-us-east-1
    memory:
      mode: STM_ONLY
      memory_id: uni_application_analyst_mem-YOUR_MEMORY_ID
      memory_arn: arn:aws:bedrock-agentcore:us-east-1:YOUR_AWS_ACCOUNT_ID:memory/uni_application_analyst_mem-YOUR_MEMORY_ID
      memory_name: uni_application_analyst_mem
      event_expiry_days: 30
      first_invoke_memory_check_done: false
      was_created_by_toolkit: false
    authorizer_configuration: null
    request_header_configuration: null
    oauth_configuration: null
